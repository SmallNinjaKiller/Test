{
  "env": {
    "es2020": true
  },
  "plugins": ["prettier", "babel"],
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "sort-imports": "error",
    "camelcase": "off",
    "no-unused-vars": "off",
    "no-unused-expressions": 0,
    "babel/no-unused-expressions": 1,
    "no-undef": "off"
  }
}
